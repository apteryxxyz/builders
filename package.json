{
    "name": "next-sa",
    "version": "0.1.2",
    "description": "Improved Next.js 13 server actions, with type-safe validation, caching and timeout",
    "packageManager": "yarn@3.6.0",
    "license": "MIT",
    "keywords": [
        "next",
        "server",
        "actions",
        "cache",
        "caching",
        "validate",
        "validation",
        "zod",
        "typescript"
    ],
    "homepage": "https://github.com/apteryxxyz/next-sa#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apteryxxyz/next-sa.git"
    },
    "bugs": {
        "url": "https://github.com/apteryxxyz/next-sa/issues"
    },
    "files": ["dist"],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./client": {
            "import": "./dist/client/index.js",
            "require": "./dist/client/index.js",
            "types": "./dist/client/index.d.ts"
        },
        "./server": {
            "import": "./dist/server/index.js",
            "require": "./dist/server/index.js",
            "types": "./dist/server/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "lint": "eslint --ext .ts,.tsx src",
        "format": "prettier --write src/**/*.{ts,tsx} && eslint --fix --ext .ts,.tsx src/"
    },
    "dependencies": {
        "enhanced-ms": "^2.3.0",
        "lru-cache": "^9.1.2",
        "object-hash": "^3.0.0",
        "zod": "^3.21.4",
        "zod-validation-error": "^1.3.0"
    },
    "peerDependencies": {
        "next": "*",
        "react": "*"
    },
    "devDependencies": {
        "@babel/types": "^7.22.4",
        "@rushstack/eslint-patch": "^1.3.0",
        "@types/eslint": "^8",
        "@types/node": "^20.2.5",
        "@types/object-hash": "^3",
        "@types/prettier": "^2",
        "@types/react": "^18",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.41.0",
        "eslint-config-apteryx": "^2.1.7",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^46.1.0",
        "eslint-plugin-n": "^16.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "prettier": "^2.8.8",
        "prettier-config-apteryx": "^2.1.0",
        "react": "^18.2.0",
        "rimraf": "^5.0.1",
        "ts-config-apteryx": "^2.1.0",
        "typescript": "<5.1.0"
    },
    "prettier": "prettier-config-apteryx"
}
