{
  "name": "next-sa",
  "version": "0.2.0",
  "description": "Improved Next.js 13 server actions, with type-safe validation and timeouts",
  "packageManager": "yarn@3.6.0",
  "license": "MIT",
  "keywords": [
    "next",
    "server",
    "actions",
    "validate",
    "validation",
    "zod",
    "typescript"
  ],
  "homepage": "https://github.com/apteryxxyz/next-sa#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apteryxxyz/next-sa.git"
  },
  "bugs": {
    "url": "https://github.com/apteryxxyz/next-sa/issues"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "client",
    "common",
    "server"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf index.js index.d.ts client common server",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "enhanced-ms": "^2.3.0",
